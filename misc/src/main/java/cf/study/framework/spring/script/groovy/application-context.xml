<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:lang="http://www.springframework.org/schema/lang"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/lang http://www.springframework.org/schema/lang/spring-lang-4.0.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd">
    
        
    <context:component-scan base-package="cf.study.framework.spring.script.groovy">
    </context:component-scan>
    
	<bean id="CONST" class="java.lang.String" scope="singleton">
		<constructor-arg value="_const"/>
	</bean>
    
	<lang:groovy id="groovyEmployeeCacheService" script-source="cf/study/framework/spring/script/groovy/GroovyEmployeeCacheService.groovy">
	</lang:groovy>    
</beans>